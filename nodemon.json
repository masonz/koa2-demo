{
    "restartable": "rs",
    "ignore": [
        ".git",
        "node_modules/**/node_modules"
    ],
    "verbose": true,
    "execMap": {
        "js": "node --harmony"
    },
    "watch": [
        "api-blueprint/",
        "*.*"
    ],
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "js json md"
}